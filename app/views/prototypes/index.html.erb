<main class="main">
  <div class="inner">
    <% if user_signed_in? %>
      <div class="greeting">
        こんにちは、
        <%= link_to current_user.name + "さん", root_path, class: :greeting__link%>
      </div>
    <% else %>
    <% end %>
    <div class="card__wrapper">
      <% @proto.each do |proto| %> 
        <%= render partial: 'prototype', locals: {proto: proto} %>
      <% end %>
    </div>
  </div>
</main>
